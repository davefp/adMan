<%= form_for [@user, @booking] do |f| %>

	<%= f.label :date %>
	<%= f.datetime :date %>
<!-- need to find a way to pass in client and page data for a drop down menu -->
	<%= f.label :name_client %>
	<%= f.text_field :name_client %>

	<%= f.label :page_name %>
	<%= f.text_field :page_name %>

	<%= f.submit "Pledge Now" %>

<% end %>